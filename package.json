{
  "name": "nodejs-react-scaffold",
  "version": "0.0.0",
  "description": "Scaffold a NodeJs and universal react project",
  "scripts": {
    "clean": "node node/clean",
    "build": "npm rebuild node-sass && gulp build",
    "start": "node node/startupExpress",
    "preclean-start": "npm i && npm run build",
    "clean-start": "node node/openBrowser",
    "postclean-start": "npm start",
    "auto-start": "nodemon node/startupExpress",
    "pre-test": "npm run build",
    "test": "mocha --compilers js:babel-register",
    "prenpp-publish": "npm i",
    "preversion": "npm test && NODE_ENV=production npm run build"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm i && npm test && gulp lint"
    }
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "i18n": "^0.8.3",
    "morgan": "^1.7.0",
    "openurl": "^1.1.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "serve-favicon": "^2.3.0",
    "swagger-jsdoc": "^1.4.0",
    "swagger-ui": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "ghooks": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-bless": "^3.1.0",
    "gulp-cached": "^1.1.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-eslint": "^3.0.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-scss-lint": "^0.4.0",
    "gulp-util": "^3.0.7",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.0",
    "webpack": "^1.13.2",
    "webpack-closure-compiler": "^2.0.2",
    "webpack-stream": "^3.2.0"
  },
  "author": "easybird"
}
